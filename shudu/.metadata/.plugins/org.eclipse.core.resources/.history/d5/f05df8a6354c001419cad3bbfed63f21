
public class tt {
public static void main(String[]args){
String s="1,2,3,4,5,6,7,8,9";
int[][] a=new int[3][3];
String[]b=s.split(",");
for(int i=0,j=0,k=0;i<b.length;i++){
a[j][k++]=Integer.parseInt(b[i]);
if(k==3){j++;k=0;}
}
//´òÓ¡³öÀ´
for(int[]c:a){
for(int n:c){
System.out.print(n+" ");
}System.out.println();
}
}
}